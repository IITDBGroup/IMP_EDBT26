select l_orderkey, sum((l_extendedprice * (1 - l_discount))::float)::float as revenue, o_orderdate, o_shippriority from ((select c_custkey from customer where c_mktsegment = 'BUILDING') customer join (select o_custkey, o_orderkey, o_orderdate, o_shippriority from orders where o_orderdate < TO_DATE('1995-03-15','YYYY-MM-DD')) orders on (c_custkey = o_custkey) join (select l_extendedprice, l_discount, l_orderkey from lineitem where l_shipdate > TO_DATE('1995-03-15','YYYY-MM-DD')) lineitem on (l_orderkey = o_orderkey)) joins group by l_orderkey, o_orderdate, o_shippriority order by revenue desc, o_orderdate limit 10
